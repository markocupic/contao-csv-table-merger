<div class="content cctm-app">
    <div id="tl_buttons">
        <a href="contao?do=csv_table_merger" class="header_back" title="" accesskey="b" onclick="Backend.getScrollOffset()">{{ 'MSC.goBack'|trans({}, 'contao_default') }}</a>
    </div>


    <div id="vueApplication" class="tl_listing_container list_view cctm-app-inner">
        <p>
            Record count: [[ record_count ]],&nbsp;
            Requests required: [[ requests_required ]],&nbsp;
            Requests remained: [[ requests_remained ]],&nbsp;
            Requests completed: [[ requests_completed ]]&nbsp;
        </p>
        <div class="cctm-summary-box" v-html="messages"></div>

    </div>

    <div class="tl_formbody_submit">
        <div class="tl_submit_container">
            <button class="tl_submit" onclick="javascript:location.href='contao?do=csv_table_merger'">{{ 'MSC.goBack'|trans({}, 'contao_default') }}</button>
        </div>
    </div>
</div>

<script>
    window.addEventListener('DOMContentLoaded', (event) => {
        const tableMergerApplication = new TableMergeApp('#vueApplication', {{ options|json_encode()|raw }});
    });


</script>
